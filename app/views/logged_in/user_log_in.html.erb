<div class="row">
	<div class="col-md-2 left-panel">
		
	</div>

	<!-- CENTRAL PANEL -->
	<div class="col-md-8 central-panel">
		<div class="row">
			<div class="col-md-5 col-md-offset-1 logged-margin-top">
				<h1>Staff</h1>
				<small>page number: </small>
			</div>
		</div>
		<div class="row">
			<!-- Dummy col -->
			<div class="col-md-1 staff-styling arrow" id="prev">
				&lArr;
			</div>
			<!-- STAFF -->
			<div id="staff-member-display">
				<% @staffs.each do |t| %>

				<div class="col-md-2 staff-styling">
					<div class="well">
						Name: <%= t.name %> <br>
						Happiness: <%= t.happiness %> <br>
						Salary: <%= t.salary %> <br>
						Performance: <%= t.performance %> <br>
						Trait: <%= t.trait_id %> <br>
					</div>

				</div>
				<% end %>
			</div>
			
			
			<div class="col-md-1 staff-styling arrow" id="next">
				&rArr;
			</div>
			
		</div>
	</div>
	<div class="col-md-2 right-panel">
		
	</div>
</div>